{
  "name": "@ldesign/grid",
  "version": "1.0.0",
  "description": "Feature-rich, high-performance GridStack wrapper for any framework",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vue": {
      "types": "./dist/adapters/vue/index.d.ts",
      "import": "./dist/adapters/vue/index.js",
      "require": "./dist/adapters/vue/index.cjs"
    },
    "./react": {
      "types": "./dist/adapters/react/index.d.ts",
      "import": "./dist/adapters/react/index.js",
      "require": "./dist/adapters/react/index.cjs"
    },
    "./lit": {
      "types": "./dist/adapters/lit/index.d.ts",
      "import": "./dist/adapters/lit/index.js",
      "require": "./dist/adapters/lit/index.cjs"
    },
    "./presets": {
      "types": "./dist/presets/index.d.ts",
      "import": "./dist/presets/index.js",
      "require": "./dist/presets/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "ldesign dev",
    "build": "ldesign build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "example:vanilla": "vite examples/vanilla",
    "example:vue": "vite examples/vue",
    "example:react": "vite examples/react",
    "example:lit": "vite examples/lit",
    "example:all": "vite examples"
  },
  "keywords": [
    "gridstack",
    "grid",
    "layout",
    "dashboard",
    "drag-and-drop",
    "vue",
    "react",
    "lit",
    "framework-agnostic",
    "typescript"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "dependencies": {
    "gridstack": "^10.3.1"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitepress": "^1.0.0-rc.36",
    "vue": "^3.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "lit": "^3.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "lit": "^2.0.0 || ^3.0.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "lit": {
      "optional": true
    }
  }
}













