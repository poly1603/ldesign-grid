{
  "name": "@ldesign/grid-vue",
  "version": "0.1.0",
  "description": "Vue 3 adapter for @ldesign/grid-core",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "dev": "vite build --watch"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "dependencies": {
    "@ldesign/grid-core": "workspace:*"
  },
  "devDependencies": {
    "vue": "^3.4.0",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.0",
    "@vitejs/plugin-vue": "^4.5.0"
  },
  "keywords": ["vue", "grid", "layout", "drag", "resize", "dashboard"],
  "license": "MIT"
}
